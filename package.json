{
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.5",
    "@nomiclabs/hardhat-etherscan": "^3.0.3",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/hardhat-upgrades": "^1.17.0",
    "@typechain/ethers-v5": "^10.0.0",
    "@typechain/hardhat": "^6.0.0",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.27",
    "chai": "^4.3.6",
    "chai-ethers": "^0.0.1",
    "dotenv": "^16.0.0",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.6.4",
    "hardhat": "^2.9.3",
    "hardhat-deploy": "^0.11.4",
    "hardhat-deploy-ethers": "^0.3.0-beta.13",
    "hardhat-gas-reporter": "^1.0.8",
    "mocha": "^9.2.2",
    "solidity-coverage": "^0.7.20",
    "ts-node": "^10.7.0",
    "typechain": "^8.0.0",
    "typescript": "^4.6.3"
  },
  "scripts": {
    "compile": "hardhat compile",
    "clean": "hardhat clean",
    "void:deploy": "hardhat deploy",
    "test": "hardhat test",
    "gas": "REPORT_GAS=true hardhat test",
    "coverage": "HARDHAT_DEPLOY_FIXTURE=true hardhat coverage",
    "fork": "hardhat node",
    "deploy:bscTestnet": "hardhat --network bscTestnet deploy",
    "deploy:bsc": "hardhat --network bsc deploy",
    "deploy:polygon": "hardhat --network polygon deploy",
    "deploy:polygonTest": "hardhat --network polygonTest deploy",
    "deploy:mainnet": "hardhat --network mainnet deploy",
    "deploy:rinkeby": "hardhat --network rinkeby deploy",
    "deploy:hardhat": "hardhat --network hardhat deploy",
    "sourcify:bscTestnet": "hardhat --network bscTestnet sourcify",
    "sourcify:polygon": "hardhat --network polygon sourcify",
    "sourcify:polygonTest": "hardhat --network polygonTest sourcify",
    "etherscan:bscTestnet": "hardhat --network bscTestnet etherscan-verify --api-key 8XVBQY25K4FDDH7D129NZR4K3T3VCCKTR5",
    "etherscan:bsc": "hardhat --network bsc etherscan-verify --api-key 8XVBQY25K4FDDH7D129NZR4K3T3VCCKTR5",
    "etherscan:polygon": "hardhat --network polygon etherscan-verify --api-key ICJVDA8H871GADXRMGM78G8STB4CUE4V8K",
    "etherscan:polygonTest": "hardhat --network polygonTest etherscan-verify --api-key ICJVDA8H871GADXRMGM78G8STB4CUE4V8K",
    "etherscan:mainnet": "hardhat --network mainnet etherscan-verify",
    "etherscan:rinkeby": "hardhat --network rinkeby etherscan-verify",
    "prestart": "yarn run typechain",
    "start": "hardhat node --network hardhat"
  },
  "dependencies": {
    "@chainlink/contracts": "^0.4.0",
    "@defi-wonderland/smock": "^2.0.7",
    "@ethersproject/contracts": "^5.6.0",
    "@openzeppelin/contracts": "^4.6.0",
    "@rari-capital/solmate": "^6.4.0",
    "@types/fs-extra": "^9.0.13",
    "files-from-path": "^0.2.6",
    "nft.storage": "^6.3.0",
    "path": "^0.12.7"
  }
}
